{
  "name": "vscode-schematics-snippets",
  "displayName": "Schematics Snippets",
  "description": "Code snippets collection for writing @angular-devkit/schematics",
  "publisher": "mikehuang",
  "version": "1.5.0",
  "scripts": {
    "build": "npm run gen:document",
    "vsce:publish": "vsce publish",
    "vsce:publish:patch": "vsce publish patch",
    "vsce:publish:minor": "vsce publish minor",
    "vsce:publish:major": "vsce publish major",
    "vsce:package": "vsce package",
    "gen:document": "ts-node document-generator.ts"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Snippets"
  ],
  "icon": "icon.png",
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./snippets/schema-snippets.json"
      },
      {
        "language": "json",
        "path": "./snippets/collection-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/schematics-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-snippets.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wellwind/vscode-schematics-snippets.git"
  },
  "license": "MIT",
  "keywords": [
    "schematics",
    "typescript",
    "code snippets"
  ],
  "devDependencies": {
    "@types/node": "^12.7.7",
    "@types/prettier": "^1.18.2",
    "@types/shelljs": "^0.8.5",
    "markdown-table": "^1.1.3",
    "shelljs": "^0.8.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "vsce": "^1.66.0"
  }
}
